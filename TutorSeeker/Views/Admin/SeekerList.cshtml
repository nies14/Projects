@model IEnumerable<TutorSeekerEntity.Seeker>

@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<ul class="breadcrumb" style="font-size:15px;color:#000;">
    <li><a href="~/Home"><i class="fa fa-dashboard  fa-fw"></i>Home</a></li>
    <li><a href="~/Admin">Admin</a></li>
    <li class="active">SeekerList </li>
    <hr class="subheaderHr" />
</ul>
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <strong>Seeker List</strong>                       
                    </div>
                    <div class="panel-body">
                        <table class="table table-condensed">
                            <tr>
                                <th>
                                   Name
                                </th>
                               
                                <th>
                                   Email
                                </th>
                                <th>
                                   Photo
                                </th>
                                <th></th>
                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.SeekerName)
                                    </td>
                                    
                                    <td>
                                        @Html.DisplayFor(modelItem => item.SeekerEmail)
                                    </td>
                                    <td>
                                        <img src="~//Upload/@Html.DisplayFor(modelItem => item.SeekerPhoto)" height="80px;" width="80px;" />
                                       
                                    </td>
                                </tr>
                            }

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>

@section scripts{
    <script src="myscript.js"></script>
    <script></script>
}

@section username{
    @Session["AdminName"]
}


@section title{
    Seeker List | Tutor Seeker
}
