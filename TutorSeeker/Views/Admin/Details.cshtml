@model TutorSeekerEntity.Admin

@{
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}


    <div>
    <ul class="breadcrumb" style="font-size:15px;color:#000;">
        <li><a href="~/Home"><i class="fa fa-dashboard  fa-fw"></i>Home</a></li>
        <li><a href="~/Admin">Admin</a></li>
        <li class="active">Details</li>
        <hr class="subheaderHr" />
    </ul>
    <div class="container">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <div class="panel panel-info">
                    <div class="panel-heading">
                        <strong>Admin List</strong>
                        <a href="/Admin/Edit/@Html.DisplayFor(model => model.AdminId)" class="pull-right"><i class="glyphicon glyphicon-edit"></i> Update Profile</a>
                    </div>
                    <div class="panel-body">
                        <table class="table table-striped">
                            <tr>

                                <th>Name</th>
                                <th>Email</th>
                                <th>Password</th>
                            </tr>
                            <tr>

                                <td>@Html.DisplayFor(model => model.AdminName)</td>
                                <td>@Html.DisplayFor(model => model.AdminEmail)</td>
                                <td>@Html.DisplayFor(model => model.AdminPassword)</td>
                            </tr>
                           

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>


    @section scripts{
        <script src="myscript.js"></script>
        <script></script>
    }

    @section username{
        @Session["AdminName"]
    }


    @section title{
        Details | Tutor Seeker
    }
